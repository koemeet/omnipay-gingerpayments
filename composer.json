{
    "name": "koemeet/omnipay-gingerpayments",
    "type": "library",
    "description": "Ginger Payments driver for Omnipay",
    "keywords": [
        "gateway",
        "ginger",
        "payments",
        "omnipay"
    ],
    "homepage": "https://github.com/koemeet/omnipay-gingerpayments",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Brem",
            "email": "steffenbrem@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\GingerPayments\\": "src/"
        }
    },
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
